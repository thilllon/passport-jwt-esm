    "JWT"
  ],
  "homepage": "https://github.com/thilllon/passport-jwt-esm/README.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/thilllon/passport-jwt-esm.git"
  },
  "bugs": {
    "url": "https://github.com/thilllon/passport-jwt-esm/issues"
  },
  "dependencies": {
    "jsonwebtoken": "^9.0.2",
    "passport-strategy": "^1.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/passport": "^1.0.16",
    "@types/passport-strategy": "^0.2.38",
    "@types/supertest": "^6.0.2",
    "chai": "^5.1.0",
    "chai-passport-strategy": "^3.0.0",
    "express": "^4.18.2",
    "jest": "^29.7.0",
    "mocha": "^10.3.0",
    "nyc": "^15.1.0",
    "passport": "^0.7.0",
    "prettier": "^3.2.5",
    "release-it": "^17.0.5",
    "sinon": "^17.0.1",
    "supertest": "^6.3.4",
    "terser": "^5.27.1",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "tsup": "^8.0.2",
    "typescript": "^5.3.3"
  }
