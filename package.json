{
  "name": "passport-jwt-esm",
  "version": "0.1.0",
  "license": "MIT",
  "description": "Passport authentication strategy using JSON Web Tokens",
  "main": "./dist",
  "types": "./dist/index.d.ts",
  "scripts": {
    "format": "prettier --write --list-different .",
